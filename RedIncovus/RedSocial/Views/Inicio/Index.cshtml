@model Entity.Publicacion

@{
    ViewBag.Title = "Que Sucede En La Red";
    List<Entity.Usuario> usuarios = ViewBag.Usuarios;
}

<script>
    $(document).ready(function () {
        CargarPublicaciones();
    })

    function CargarPublicaciones() {
        $("#Publicaciones").html('<blink>Cargando ..</blink>');
        $("#Publicaciones").load(base_url('inicio/Publicaciones'));
    }
</script>

<div class="page-header remove-margin-top">
    <h1>Que Sucede En La Red</h1>
</div>

<div id="Publicaciones"></div>